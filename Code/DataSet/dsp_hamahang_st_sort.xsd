                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        SUM(derivedtbl_3.scoresum * derivedtbl_3.vahed) / SUM(derivedtbl_3.vahed) AS miangin, student_1.name AS stu_name, student_1.lname AS stu_lname, 
                         student_1.fname, derivedtbl_3.idstudent, class.name AS clasname
FROM            ((((SELECT        AVG(score_1.score) AS scoresum, score_1.idstudent, lesson_1.vahed, derivedtbl_1_1.idlesson
                              FROM            (((SELECT        test_1.idtest, test_1.idlesson, test_1.[date], test_1.idclass
                                                           FROM            (hamahang_test INNER JOIN
                                                                                    test test_1 ON hamahang_test.idtest = test_1.idtest)
                                                           WHERE        (hamahang_test.id = ?)
                                                           GROUP BY test_1.idtest, test_1.idlesson, test_1.[date], test_1.idclass) derivedtbl_1_1 INNER JOIN
                                                        lesson lesson_1 ON derivedtbl_1_1.idlesson = lesson_1.idlesson) INNER JOIN
                                                        score score_1 ON derivedtbl_1_1.idtest = score_1.idtest)
                              GROUP BY score_1.idstudent, lesson_1.vahed, derivedtbl_1_1.idlesson) derivedtbl_3 INNER JOIN
                         student student_1 ON derivedtbl_3.idstudent = student_1.idstudent) INNER JOIN
                             (SELECT        student_2.idstudent, class_1.idclass
                               FROM            (student student_2 LEFT OUTER JOIN
                                                         class class_1 ON student_2.idclass = class_1.idclass)) derivedtbl_1 ON derivedtbl_3.idstudent = derivedtbl_1.idstudent) INNER JOIN
                         class ON derivedtbl_1.idclass = class.idclass)
WHERE        (derivedtbl_1.idclass = ?)
GROUP BY student_1.name, student_1.lname, student_1.fname, derivedtbl_3.idstudent, class.name
ORDER BY SUM(derivedtbl_3.scoresum * derivedtbl_3.vahed) / SUM(derivedtbl_3.vahed) DESC</CommandText>
