                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT     lesson.name, AVG(score.score) AS angclass, class.name AS classname
FROM         (((score INNER JOIN
                      test ON score.idtest = test.idtest) INNER JOIN
                      lesson ON test.idlesson = lesson.idlesson) INNER JOIN
