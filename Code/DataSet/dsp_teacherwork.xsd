                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>SELECT     lesson_teacher.idteacher, lesson.name, lesson.grade, major.name AS majorname, student.lname, student.name AS studentname, COUNT(score.score) 
                      AS scorecount, class.name AS classname
FROM         (lesson_teacher RIGHT OUTER JOIN
                      (major RIGHT OUTER JOIN
                      (((score LEFT OUTER JOIN
                      (class RIGHT OUTER JOIN
                      student ON class.idclass = student.idclass) ON score.idstudent = student.idstudent) RIGHT OUTER JOIN
                      test ON score.idtest = test.idtest) RIGHT OUTER JOIN
                      lesson ON test.idlesson = lesson.idlesson) ON major.idmajor = lesson.idmajor) ON lesson_teacher.idlesson = lesson.idlesson)
GROUP BY lesson.name, major.name, lesson_teacher.idteacher, lesson.grade, student.lname, student.name, class.name
HAVING      (lesson_teacher.idteacher = ?)
ORDER BY student.lname, student.name</CommandText>
